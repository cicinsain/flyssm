lines=("136" "137" "138" "139" "141" "142" "143" "144")
counter=$1
outputfile=$2
dir=${2%/*}
# outputfile=tllg58c13-v4.2_001_ggn_nd_pSize_400_nGen_400_pCross_0.7_pMut_0.025.inp_outputs/tllg58c13-v4.2_001_ggn_nd_pSize_400_nGen_400_pCross_0.7_pMut_0.025.inp_parm_${counter}.fout
touch $dir/$counter.network
for line in ${lines[@]}; do
	row=($(sed -n "${line}p" < ${outputfile}))
	echo ${row[@]} >> $dir/${counter}.network

done
